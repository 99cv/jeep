{
  "name": "pwa-components-viewme",
  "private": true,
  "version": "0.0.4-1",
  "description": "Jeep Components Library Viewme PWA App",
  "keywords": [
    "jeep",
    "stencil",
    "progressive web app",
    "pwa",
    "app",
    "view me"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "update": "npm install --save @jeepq/core@latest --save-dev @stencil/core@latest",
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "start.sw": "npm run start --service-worker",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "generate": "stencil generate",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@ionic/core": "^5.0.5"
  },
  "devDependencies": {
    "@jeepq/core": "0.0.4-1",
    "@stencil/core": "^1.9.2",
    "@types/jest": "^24.0.25",
    "@types/puppeteer": "^1.20.3",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "puppeteer": "1.20.0",
    "workbox-build": "4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepiqueau/jeep.git"
  },
