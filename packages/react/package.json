    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepiqueau/jeep.git"
  },
  "author": "Jean Pierre Qu√©au",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jepiqueau/jeep/issues"
  },
  "homepage": "https://github.com/jepiqueau/jeep#readme",
