{
  "name": "@jeepq/react",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "React specific wrapper for @jeepq/core",
  "keywords": [
    "jeep",
    "react",
    "web component",
    "web component library"
  ],
  "scripts": {
    "update": "npm install --save @jeepq/core@latest",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "deploy": "npm run build && npm publish --access public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@jeepq/core": "0.0.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepiqueau/jeep.git"
  },
  "author": "Jean Pierre Qu√©au",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jepiqueau/jeep/issues"
  },
  "homepage": "https://github.com/jepiqueau/jeep#readme",
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
